<% produce.each do |prod| %>
<div clas="row">
  <div class="col-md-6 text-center">
    <h2 align='center'><%= prod.produce.name %></h2>
    <div class="produce-img">
      <%= link_to image_tag(prod.produce.image.thumb.url), produce_path(prod.produce) %>
    </div><br>
    <%= expires(prod) %>
    <br></br>
    <%= button_to "I ate it!", eaten_path(prod), class: 'btn btn-default' %>
  </div>
</div>
<% end %>
